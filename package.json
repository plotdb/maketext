{
  "author": "zbryikt",
  "name": "servebase",
  "license": "MIT",
  "description": "base server for constructing services",
  "version": "0.0.1",
  "homepage": "https://github.com/plotdb/servebase",
  "repository": {
    "type": "git",
    "url": "https://github.com/plotdb/servebase"
  },
  "engines": {
    "node": ">=15.11.0",
    "npm": ">=7.0.15"
  },
  "workspaces": [
    "module/auth",
    "backend/engine"
  ],
  "scripts": {
    "start": "NODE_ENV=production ./start",
    "dev": "./start",
    "demo": "./start demo",
    "try": "./node_modules/.bin/lsc log.ls",
    "prebuild": "node_modules/.bin/lsc -co .backend backend",
    "config": "cd config; ./build",
    "bootstrap": "npx bootstrap.build -c config/bootstrap/ -o static/assets/lib/bootstrap.custom/main/",
    "docker-start": "docker-compose -f config/docker/compose.yaml up",
    "docker-stop": "docker-compose -f config/docker/compose.yaml down",
    "docker-build": "docker-compose -f config/docker/compose.yaml build",
    "docker-db": "docker-compose -f config/docker/compose.yaml up -d db",
    "cannon": "cd tool; ./cannon",
    "test": "npx mocha --require livescript ./test/index.ls",
    "coverage": "npx nyc --reporter=text npm run test"
  },
  "devDependencies": {
    "colors": "^1.3.3",
    "express": "=4.17.1",
    "stylus": "^0.54.5",
    "uglify-js": "=3.2.0",
    "uglifycss": "^0.0.29"
  },
  "dependencies": {
    "@plotdb/srcbuild": "^0.0.30",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "chokidar": "^3.4.2",
    "csurf": "^1.11.0",
    "curegex": "^0.0.3",
    "express-list-endpoints": "^5.0.0",
    "express-session": "^1.17.1",
    "i18next": "^19.8.3",
    "i18next-fs-backend": "^1.0.7",
    "i18next-http-middleware": "^3.0.6",
    "js-yaml": "^3.14.0",
    "jsonwebtoken": "^8.5.1",
    "lderror": "^1.1.5",
    "livescript": "^1.6.0",
    "ls-lint": "^0.1.2",
    "marked": "^2.0.1",
    "mocha": "^8.2.1",
    "nodemailer": "^6.7.0",
    "nodemailer-mailgun-transport": "^2.1.3",
    "nyc": "^15.1.0",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-line-auth": "^0.2.9",
    "passport-local": "^1.0.0",
    "pg": "^8.3.3",
    "pg-pool": "^3.2.1",
    "pino": "^6.6.0",
    "pino-http": "^5.2.0",
    "pino-pretty": "^4.2.1",
    "proxise": "^0.1.2",
    "pug": "^3.0.0",
    "re2": "^1.15.9",
    "redis": "^3.0.2",
    "redis-clustr": "^1.4.3",
    "require-reload": "^0.2.2",
    "template-text": "^0.1.2",
    "yargs": "^16.2.0"
  }
}
